<html>
	<head>
		<title>W04: Task 2</title>
	</head>
	<body>
		<script src="https://d3js.org/d3.v4.min.js"></script>
		<script>
			write.document("Label1");
write.document("Label2");
write.document("Label3");
var svg = d3.select("body").append("svg");
d3.csv("https://takahiroyoshikura.github.io/InfoVis2021/W04/w04_task2.csv",function(data){
	svg.selectAll("rect")
	.data(data)
	.enter()
	.append("rect")
	.attr("x",100)
		.attr("y",function(d,i){return 10 + i * 30;})
		.attr("width",function(d){return d.width;})
	.attr("height",20)
});
		</script>
	</body>
</html>


